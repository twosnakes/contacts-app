<h1>Contact Info</h1>
<h2>First Name: <%= @contact.first_name %></h2>
<h2>Middle Name: <%= @contact.middle_name %></h2>
<h2>Last Name: <%= @contact.last_name %></h2>
<h2>Email: <%= @contact.email %></h2>
<h2>Phone: <%= @contact.phone_number %></h2>

<%= link_to "Delete Contact", "/contacts/#{ @contact.id }", method: :delete %>
<%= link_to "Edit Contact", "/contacts/#{ @contact.id }/edit" %>




